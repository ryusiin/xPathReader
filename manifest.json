{
  "manifest_version": 3,
  "name": "xPath Reader",
  "version": "1.1",
  "description": "This is xPath Reader!",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["activeTab"]
}